<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_family_view_tree" model="ir.ui.view">
        <field name="name">partner family view tree</field>
        <field name="model">partner.family</field>
        <field name="arch" type="xml">
